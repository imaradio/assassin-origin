{
	"type": "origins:multiple",
	"name": "Flexible Movement",
	"description": "You find it much easier to travel around than others, but anything better than iron armour is constricts your movement somewhat.",

	"attributes": {
		"type": "origins:conditioned_attribute",
		"modifier": {
			"attribute": "minecraft:generic.movement_speed",
			"operation": "addition",
			"value": 0.04,
			"name": "higher sprint speed, assassin origin"
		},
		"tick_rate": 4,
		"condition": {
			"type": "origins:or",
			"conditions": [
				{
					"type": "origins:equipped_item",
					"equipment_slot": "head",
					"item_condition": {
						"type": "origins:armor_value",
						"comparison": ">",
						"compare_to": 2
					}
				},
				{
					"type": "origins:equipped_item",
					"equipment_slot": "chest",
					"item_condition": {
						"type": "origins:armor_value",
						"comparison": ">",
						"compare_to": 6
					}
				},
				{
					"type": "origins:equipped_item",
					"equipment_slot": "legs",
					"item_condition": {
						"type": "origins:armor_value",
						"comparison": ">",
						"compare_to": 5
					}
				},
				{
					"type": "origins:equipped_item",
					"equipment_slot": "feet",
					"item_condition": {
						"type": "origins:armor_value",
						"comparison": ">",
						"compare_to": 2
					}
				}
			],
			"inverted": true
		}
	},

  "double_jump_bool": {
    "type": "origins:resource",
    "min": 0,
    "max": 1,
    "hud_render": {}
  },
  "double_jump": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:add_velocity",
          "y": 0.45,
          "set": true
        },
        {
          "type": "origins:add_velocity",
          "z": 0.25,
          "space": "local_horizontal"
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_double_jump_bool",
          "change": 1
        }
      ]
    },
    "key": {
      "key": "key.jump"
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_double_jump_bool",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:on_block",
          "inverted": true
        },
        {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:equipped_item",
              "equipment_slot": "head",
              "item_condition": {
                "type": "origins:armor_value",
                "comparison": ">",
                "compare_to": 2
              }
            },
            {
              "type": "origins:equipped_item",
              "equipment_slot": "chest",
              "item_condition": {
                "type": "origins:armor_value",
                "comparison": ">",
                "compare_to": 6
              }
            },
            {
              "type": "origins:equipped_item",
              "equipment_slot": "legs",
              "item_condition": {
                "type": "origins:armor_value",
                "comparison": ">",
                "compare_to": 5
              }
            },
            {
              "type": "origins:equipped_item",
              "equipment_slot": "feet",
              "item_condition": {
                "type": "origins:armor_value",
                "comparison": ">",
                "compare_to": 2
              }
            }
          ],
          "inverted": true
        }
      ]
    }
  },
  "double_jump_bool_reset_on_land": {
    "type": "origins:action_on_land",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_double_jump_bool",
      "change": -1
    }
  },
  "double_jump_bool_reset_on_climb": {
    "type": "origins:action_over_time",
    "rising_action": {
      "type": "origins:change_resource",
      "resource": "*:*_double_jump_bool",
      "change": -1
    },
    "interval": 2,
    "condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:or",
					"conditions": [
						{
							"type": "origins:block_collision",
							"offset_x": 0.001,
							"offset_z": 0.001
						},
						{
							"type": "origins:block_collision",
							"offset_x": -0.001,
							"offset_z": -0.001
						}
					]
				},
				{
					"type": "origins:power_active",
					"power": "*:master_rock_climber_toggle"
				}
			]
		}
  },


	"fall_invulnerability": {
		"type": "origins:invulnerability",
		"damage_condition": {
			"type": "origins:name",
			"name": "fall"
		}
	}
}
