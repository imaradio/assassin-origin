{
	"type": "origins:multiple",

	"toggle": {
		"type": "origins:toggle",
		"key": {
			"key": "key.origins.secondary_active"
		}
	},
	"climbing": {
		"type": "origins:climbing",
    "hold_condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:or",
					"conditions": [
						{
							"type": "origins:block_collision",
							"offset_x": 0.001,
							"offset_z": 0.001
						},
						{
							"type": "origins:block_collision",
							"offset_x": -0.001,
							"offset_z": -0.001
						}
					]
				},
				{
					"type": "origins:power_active",
					"power": "*:*_toggle"
				}
			]
		},
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:or",
					"conditions": [
						{
							"type": "origins:block_collision",
							"offset_x": 0.001,
							"offset_z": 0.001
						},
						{
							"type": "origins:block_collision",
							"offset_x": -0.001,
							"offset_z": -0.001
						}
					]
				},
				{
					"type": "origins:power_active",
					"power": "*:*_toggle"
				}
			]
		}
	},

	"name": "Master Rock Climber",
	"description": "Having to climb numerous heights to reach your victims, you've learnt to climb well."
}
